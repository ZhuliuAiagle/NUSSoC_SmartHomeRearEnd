<?xml version='1.0' encoding='UTF-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.zijinshen.datadef.DeviceCondition" table="devicecondition">
        <id name="conditionId">
            <generator class="assigned"></generator>
        </id>
        <property name="fanOpen"></property>
        <property name="fanLevel"></property>
        <property name="lightOpen"></property>
        <property name="lightLevel"></property>
    </class>
</hibernate-mapping>